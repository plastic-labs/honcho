{
    "sessions": {
        "session1": {
            "messages": [
                {
                    "peer": "alice",
                    "content": "Hello, how are you?"
                },
                {
                    "peer": "bob",
                    "content": "I'm good, thank you!"
                }
            ]
        },
        "session2": {
            "messages": [
                {
                    "peer": "alice",
                    "content": "Hello, how are you?"
                },
                {
                    "peer": "bob",
                    "content": "I'm doing terribly :("
                }
            ]
        }
    },
    "queries": [
        {
            "observer": "alice",
            "query": "How is Bob doing?",
            "session": "session1",
            "target": "bob",
            "expected_response": "Good"
        },
        {
            "observer": "alice",
            "query": "How is Bob doing?",
            "session": "session2",
            "target": "bob",
            "expected_response": "Terribly"
        },
        {
            "observer": "bob",
            "query": "How is Bob doing?",
            "session": "session1",
            "expected_response": "Good"
        },
        {
            "observer": "bob",
            "query": "How is Bob doing?",
            "session": "session2",
            "expected_response": "Terribly"
        }
    ]
}